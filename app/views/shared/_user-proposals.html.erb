<div class="tab-content-container">
  <div class="row">
    <% if user.proposals.size == 0 %>
      <p>Vous n'avez fait aucune proposition</p>
    <% else %>
      <% user.proposals.each do |proposal| %>
        <% if proposal.photo.nil? %>
          <p>Pas de photo</p>
        <% else %>
          <%= proposal.photo %>
        <% end %>
      <% end %>
    <% end %>
  </div>
</div>

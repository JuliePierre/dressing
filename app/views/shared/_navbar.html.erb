<nav>
  <div class="nav-wrapper">
    <div class="container">
      <%= link_to 'Wedding Dressing', root_path, class: 'brand-logo' %>
      <a href="#" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
      <ul class="right hide-on-med-and-down">
        <% if current_user %>
          <li>
            <%= link_to dashboard_user_path(current_user) do %>
              <div class='dropdown-button btn', href='#', data-activates='dropdown1'>Me</div>
            <% end %>
          </li>
          <!-- Dropdown Structure -->
            <ul id='dropdown1' class='dropdown-content'>
              <li><%= link_to 'Mon compte', dashboard_user_path(current_user) %></li>
              <li><%= link_to "Mes vide dressings", user_vide_dressings_path(current_user) %>
            </ul>
        <% else %>
          <li><%= link_to 'Log in', new_user_session_path %></li>
        <% end %>

      </ul>
    </div>
  </div>
</nav>
<ul class="side-nav" id="mobile-demo">
  <% if current_user %>
    <li><%= link_to 'Mon compte', dashboard_user_path(current_user) %></li>
  <% else %>
    <li><%= link_to 'Log in', new_user_session_path %></li>
  <% end %>
</ul>


<% content_for(:after_js) do %>
  <%= javascript_tag do %>
    $(document).ready(function() {
      $(".button-collapse").sideNav();
    });
  <% end %>
<% end %>

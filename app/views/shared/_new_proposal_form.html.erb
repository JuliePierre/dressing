<%= simple_form_for(proposal) do |f| %>
  <%= f.error_notification %>
  <%= f.input :missing_item, as: :check_boxes, collection: MissingItem::ITEMS %>

  <%= f.file_field :photo, :input_html => {:accept => "image/*", :capture => "capture"} %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= hidden_field_tag(:outfit_id, @outfit.id) %>
  <%= f.button :submit, class: 'highlight-btn' %>
<% end %>

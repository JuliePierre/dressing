<%= simple_form_for(proposal) do |f| %>
  <%= f.error_notification %>
  <%= f.input :missing_item, as: :check_boxes, collection: MissingItem::CATEGORY %>

  <%= f.input :photos, as: :attachinary, :input_html => {:accept => "image/*", :capture => "capture"} %>

  <%= hidden_field_tag(:outfit_id, @outfit.id) %>
  <%= f.button :submit, class: 'highlight-btn' %>
<% end %>

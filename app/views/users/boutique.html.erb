<div class="main-body bg-grey">
  <div class="dashboard-content-header underlined">
    <h4>Ma boutique</h4>
  </div>
  <ul class="top-nav-tabs boutique-nav-tabs">
    <li class="tab top-nav-tab boutique-nav-tab"><a href="#tab-articles" class="active">Mes articles <span class="notification-pin"></a></li>
    <li class="tab top-nav-tab"><a href="#tab-emprunts">Emprunts <span class="notification-pin">( <%= @pending_loans.size %> )</span></a></li>
    <div class="indicator" style="right: 1050px; left: 30px;"></div>
  </ul>
  <!-- TAB MES ARTICLES -->
  <%= render "users/boutique_tabs/articles" %>
  <!-- TAB MES EMPRUNTS -->
  <%= render "users/boutique_tabs/emprunts" %>






</div>
<% content_for(:after_js) do %>
  <script>
    $(document).ready(function(){
      $('ul.boutique-nav-tabs').tabs();
    });
  </script>
<% end %>

<div class="main-body">
  <div class="page-title bg-grey">
    <div class="container">
      <h1>All outfits</h1>
    </div>
  </div>
  <div class="container">
    <div class="padded">
      <div class="row">
        <% @outfits.each do |outfit| %>
          <%= render 'shared/outfit_card', outfit: outfit, picture: outfit.photos[0] %>
        <% end %>
      </div>
    </div>
  </div>
</div>
